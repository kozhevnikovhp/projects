                    END
                    POPUP "PSM-500"
                    BEGIN
                        MENUITEM "L-band",                      ID_DEVICE_ADD_MODEM_DATUM_PSM500_LBAND

                    END
                    POPUP "M7"
                    BEGIN
                        MENUITEM "L-Band modulator + L-band demodulator", 
                                                                ID_DEVICE_ADD_MODEM_DATUM_M7_LMOD_LDEM

                    END
                END
                POPUP "Radyne"
                BEGIN
                    MENUITEM "DMD-2401",                    ID_DEVICE_ADD_MODEM_RADYNE_DMD2401

                END
            END
            POPUP "Tranceiver"
            BEGIN
                POPUP "WaveSat"
                BEGIN
                    MENUITEM "WTR-70",                      ID_DEVICE_ADD_TRANCEIVER_WAVESAT_WTR70

                    MENUITEM "WaveSat Substitutor",         ID_DEVICE_ADD_TRANCEIVER_WAVESAT_WAVESATSUBSTITUTOR

                END
                POPUP "Advantech"
                BEGIN
                    MENUITEM "WTY-1",                       ID_DEVICE_ADD_TRANCEIVER_ADVANTECH_WTY1

                END
                POPUP "Vertex"
                BEGIN
                    MENUITEM "50 Watt",                     ID_DEVICE_ADD_TRANCEIVER_VERTEX_50WATT

                    MENUITEM "Vertex substitutor",          ID_DEVICE_ADD_TRANCEIVER_VERTEX_VERTEX_SUBSTITUTOR

                END
            END
            POPUP "Up/Down converter"
            BEGIN
                POPUP "Up Converter KVV-140"
                BEGIN
                    MENUITEM "ver. 2002",                   ID_DEVICE_ADD_UPDOWNCONVERTER_UPCONVERTERKVV140_VER2002

                    MENUITEM "ver. 2004",                   ID_DEVICE_ADD_UPDOWNCONVERTER_UPCONVERTERKVV140_VER2004

                    MENUITEM "ver. 2011",                   ID_DEVICE_ADD_UPDOWNCONVERTER_UPCONVERTERKVV140_VER2011

                END
                POPUP "Down Converter KVN-140"
                BEGIN
                    MENUITEM "ver. 2002",                   ID_DEVICE_ADD_UPDOWNCONVERTER_DOWNCONVERTERKVN140_VER2002

                    MENUITEM "ver. 2004",                   ID_DEVICE_ADD_UPDOWNCONVERTER_DOWNCONVERTERKVN140_VER2004

                    MENUITEM "ver. 2011",                   ID_DEVICE_ADD_UPDOWNCONVERTER_DOWNCONVERTERKVN140_VER2011

                END
            END
            POPUP "SDMS"
            BEGIN
                POPUP "SDMS-VSAT"
                BEGIN
                    MENUITEM "4900L",                       ID_DEVICE_ADD_SDMS_SDMSVSAT_4900L

                    MENUITEM "M500L",                       ID_DEVICE_ADD_SDMS_SDMSVSAT_M500L

                END
            END
            POPUP "Power Meter"
            BEGIN
                POPUP "Agilent"
                BEGIN
                    MENUITEM "E4418B",                      ID_DEVICE_ADD_POWERMETER_AGILENT_E4418B

                END
            END
            POPUP "Frequency Counter"
            BEGIN
                POPUP "Agilent"
                BEGIN
                    MENUITEM "53150A",                      ID_DEVICE_ADD_FREQUENCYCOUNTER_AGILENT_53150F

                END
            END
            POPUP "Access Device"
            BEGIN
